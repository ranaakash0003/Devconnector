{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "eqeqeq":"warn",
    "prefer-const":"warn",
    "spaced-comment":"off",
    "prefer-template":"off",
    "no-else-return":"off",
    "no-shadow":"off",
    "consistent-return": "off",
    "no-underscore-dangle": 0,
    "prettier/prettier": "off",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  }
}
